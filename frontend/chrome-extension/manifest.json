{
  "name": "System Management Extension",
  "description": "Linux system management tool",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Systems Management",
    "default_icon": {
      "16": "./images/favicon.png",
      "32": "./images/favicon.png",
      "48": "./images/favicon.png",
      "128": "./images/favicon.png"
    }
  },
  "icons": {
    "16": "./images/favicon.png",
    "32": "./images/favicon.png",
    "48": "./images/favicon.png",
    "128": "./images/favicon.png"
  }
}
